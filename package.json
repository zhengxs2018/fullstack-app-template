{
  "name": "fullstack-app-template",
  "private": true,
  "version": "0.0.1",
  "description": "koa 应用模板",
  "keywords": [
    "koa",
    "app"
  ],
  "main": "./app/main.js",
  "scripts": {
    "start": "node -r ./setup.js ./app/main.js",
    "dev": "nodemon -r ./setup.js ./app/main.js",
    "debug": "nodemon --inspect -r ./setup.js ./app/main.js",
    "debug:prod": "node --inspect -r ./setup.js ./app/main.js",
    "lint": "npm run lint:code && npm run lint:resource",
    "lint:code": "eslint app/**/*.js *.js --fix",
    "lint:resource": "prettier \"app/**/*.{json,yaml}\" *.json --write",
    "db:up": "npm run db:client:gen && npm run db:migrate:gen && npm run db:migrate:up",
    "db:client:gen": "prisma generate",
    "db:migrate:gen": "prisma migrate save --name app --experimental",
    "db:migrate:up": "prisma migrate up --experimental",
    "db:studio": "npx prisma studio  --experimental"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@koa/router": "^9.0.1",
    "@prisma/client": "^2.0.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "ioredis": "^4.17.3",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-redis": "^4.0.1",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^3.0.1",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "nunjucks": "^3.2.1",
    "parameter": "^3.6.0",
    "sqlite": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@prisma/cli": "^2.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/ioredis": "^4.16.5",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/lodash": "^4.14.155",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.13",
    "@types/nunjucks": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "chokidar": "^3.4.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "swagger-jsdoc": "^4.0.0"
  },
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x"
  },
  "author": {
    "name": "zhengxs",
    "email": "zhengxs2018@foxmail.com"
  },
  "license": "MIT"
}
